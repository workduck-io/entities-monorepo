{
  "root": "stacks/crud",
  "projectType": "application",
  "sourceRoot": "stacks/crud/src",
  "targets": {
    "build": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "cwd": "stacks/crud",
        "color": true,
        "command": "sls package"
      }
    },
    "serve": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "cwd": "stacks/crud",
        "color": true,
        "command": "sls offline start"
      }
    },
    "deploy": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "cwd": "stacks/crud",
        "color": true,
        "command": "sls deploy --verbose"
      }
    },
    "remove": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "cwd": "stacks/crud",
        "color": true,
        "command": "sls remove"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["stacks/crud/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/stacks/crud"],
      "options": {
        "jestConfig": "stacks/crud/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": ["stack"]
}
